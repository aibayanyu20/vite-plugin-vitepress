{
  "name": "vite-plugin-vitepress",
  "version": "0.0.1-alpha.1",
  "description": "this is base vite parser markdown for vitepress",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "theme.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./theme": {
      "import": "./dist/theme/index.mjs",
      "require": "./dist/theme/index.js",
      "types": "./dist/theme.d.ts"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "scripts": {
    "test": "vitest -u",
    "dev:example": "vite dev example",
    "build:example": "vite build example",
    "preview:example": "vite preview example",
    "release": "bumpp",
    "build": "tsup"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@mistjs/eslint-config-vue": "^0.0.3",
    "@mistjs/tsconfig-vue": "^0.0.3",
    "@types/node": "^18.11.11",
    "@vitejs/plugin-vue": "^3.2.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.29.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.5",
    "vite-plugin-inspect": "^0.7.9",
    "vitepress": "1.0.0-alpha.30",
    "vitest": "^0.25.5",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "peerDependencies": {
    "vitepress": "1.0.0-alpha.30"
  }
}
